<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.cache.mapper.CacheMapper">
  <select id="selectCacheById" parameterType="String" resultType="com.example.cache.entity.response.CacheResponse">
    SELECT * FROM caches WHERE id=#{id}
